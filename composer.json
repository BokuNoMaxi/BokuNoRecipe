{
    "name": "boku-no-recipe/bokunorecipe",
    "type": "typo3-cms-extension",
    "description": "This extension is for storing recipes in Typo3",
    "authors": [
        {
            "name": "Markus Ketterer",
            "role": "Developer"
        }
    ],
    "license": "GPL-2.0-or-later",
    "require": {
        "typo3/cms-core": "^10.4"
    },
    "require-dev": {
        "typo3/testing-framework": "^6.8"
    },
    "autoload": {
        "psr-4": {
            "BokuNoRecipe\\Bokunorecipe\\": "Classes"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BokuNoRecipe\\Bokunorecipe\\Tests\\": "Tests"
        }
    },
    "replace": {
        "typo3-ter/bokunorecipe": "self.version"
    },
    "config": {
        "vendor-dir": ".Build/vendor",
        "bin-dir": ".Build/bin"
    },
    "scripts": {
        "post-autoload-dump": [
            "TYPO3\\TestingFramework\\Composer\\ExtensionTestEnvironment::prepare"
        ]
    },
    "extra": {
        "typo3/cms": {
            "cms-package-dir": "{$vendor-dir}/typo3/cms",
            "web-dir": ".Build/public",
            "extension-key": "bokunorecipe"
        }
    }
}